        <splash height="300" src="resources/windows/splash/Splash620x300.scale-100.png" width="620" />
        <icon height="150" src="resources/windows/icon/logo150x150.scale-100.png" width="150" />
        <icon height="310" src="resources/windows/icon/logo310x310.scale-100.png" width="310" />
        <icon height="44" src="resources/windows/icon/logo44x44.scale-100.png" width="44" />
