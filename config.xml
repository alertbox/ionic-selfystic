        <icon height="71" src="resources/windows/icon/logo71x71.scale-100.png" width="71" />
        <icon height="150" src="resources/windows/icon/wide310x150.scale-100.png" width="310" />
    </platform>
    <plugin name="cordova-plugin-camera" version="2.3.0" />
    <plugin name="cordova-plugin-code-push" version="1.9.2-beta" />
    <preference name="WindowsStoreDisplayName" value="Selfystic" />
    <vs:platformSpecificValues xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
        <vs:platformSpecificWidget platformName="windows" version="0.1.0.0" />
    </vs:platformSpecificValues>
    <preference name="target-device" value="handset" />
    <engine name="android" spec="latest" />
</widget>
